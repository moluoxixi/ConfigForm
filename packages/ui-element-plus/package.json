{
  "name": "@moluoxixi/ui-element-plus",
  "type": "module",
  "version": "0.0.1",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --tsconfig tsconfig.build.json",
    "dev": "tsup src/index.ts --format esm,cjs --dts --tsconfig tsconfig.build.json --watch"
  },
  "peerDependencies": {
    "element-plus": ">=2.7.0",
    "vue": ">=3.5.0"
  },
  "dependencies": {
    "@moluoxixi/core": "workspace:*",
    "@moluoxixi/ui-vue-shared": "workspace:*",
    "@moluoxixi/vue": "workspace:*"
  },
  "devDependencies": {
    "element-plus": "catalog:build",
    "tsup": "catalog:type",
    "typescript": "catalog:type",
    "vue": "catalog:build"
  }
}
