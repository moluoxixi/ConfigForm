  "peerDependencies": {
    "vue": ">=3.5.0"
  },
  "dependencies": {
    "@moluoxixi/core": "workspace:*",
    "@moluoxixi/reactive-vue": "workspace:*",
    "@moluoxixi/schema": "workspace:*"
  },
  "devDependencies": {
    "tsup": "catalog:type",
    "typescript": "catalog:type",
    "vue": "catalog:build"
  }
}
