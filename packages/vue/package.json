{
  "name": "@moluoxixi/vue",
  "type": "module",
  "version": "0.0.1",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": ["dist", "src"],
