{
  "name": "@moluoxixi/ui-antd",
  "type": "module",
  "version": "0.0.1",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch"
  },
  "peerDependencies": {
    "antd": ">=5.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@moluoxixi/core": "workspace:*",
    "@moluoxixi/react": "workspace:*",
    "@moluoxixi/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "catalog:type",
    "@types/react-dom": "catalog:type",
    "antd": "catalog:build",
    "react": "catalog:build",
    "react-dom": "catalog:build",
    "tsup": "catalog:type",
    "typescript": "catalog:type"
  }
}
